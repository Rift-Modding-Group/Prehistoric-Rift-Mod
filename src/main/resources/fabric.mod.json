{
  "schemaVersion": 1,
  "id": "rift",
  "version": "${version}",

  "name": "Prehistoric Rift",
  "description": "This is a port of the Minecraft Bedrock addon Prehistoric Rift. It aims to add dinosaurs and expand upon gameplay.",
  "authors": [
    "ANightDazingZoroark"
  ],
  "contact": {
    "homepage": "https://anightdazingzoroark.github.io/",
    "sources": "https://github.com/ANightDazingZoroark/Prehistoric-Rift-Mod"
  },

  "license": "CC0-1.0",
  "icon": "assets/rift/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "com.anightdazingzoroark.rift.InitializeServer"
    ],
    "client": [
      "com.anightdazingzoroark.rift.InitializeClient"
    ]
  },

  "depends": {
    "fabricloader": ">=0.11.3",
    "fabric": "*",
    "minecraft": "1.17.x",
    "java": ">=16"
  },
  "suggests": {
    "another-mod": "*"
  },
  "mixins": [
    "rift.mixins.json"
  ]
}
